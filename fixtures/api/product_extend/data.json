    "endpoints": {
        "get": {
            "method": "GET",
            "url": {
                "en": "\/productExtend\/product id"
            },
            "description": {
                "en": "Used to retrieve a specific item."
            },
            "parameters": [],
            "example": {
                "url": "\/productExtend\/cHJvZHVjdC1wcm9kdWN0X2lkPTUw",
                "statusCode": "200"
            }
        },
        "getCollection": {
            "method": "GET",
            "url": {
                "en": "\/productExtend"
            },
            "description": {
                "en": "Used to retrieve a collection of elements."
            },
            "parameters": [
                {
                    "name": "page",
                    "description": {
                        "en": "The serial number of the given page."
                    }
                },
                {
                    "name": "limit",
                    "description": {
                        "en": "Number of items displayed on a page. Maximum 200."
                    }
                },
                {
                    "name": "full",
                    "description": {
                        "en": "Whether the values of the first-level fields should be expanded. Value: 1 (yes) or 0 (no). Default: 0."
                    }
                },
                {
                    "name": "innerId",
                    "description": {
                        "en": "Search parameter. Product internal identifier."
                    }
                },
                {
                    "name": "sku",
                    "description": {
                        "en": "Search parameter. Product sku."
                    }
                },
                {
                    "name": "createdAt",
                    "description": {
                        "en": "Search parameter. It can be used for search the creation date. Returns products created on the specified date. Format: 2013-01-01."
                    }
                },
                {
                    "name": "createdAfter",
                    "description": {
                        "en": "Search parameter. It can be used for search the creation date. Returns products created after the specified date. Format: 2019-01-01T00:00:00."
                    }
                },
                {
                    "name": "createdBefore",
                    "description": {
                        "en": "Search parameter. It can be used for search the creation date. Returns products created before the specified date. Format: 2019-01-01T00:00:00."
                    }
                },
                {
                    "name": "updatedAt",
                    "description": {
                        "en": "Search parameter. It can be used for search the modification date. Returns products modified at the specified date. Format: 2013-01-01."
                    }
                },
                {
                    "name": "updatedAfter",
                    "description": {
                        "en": "Search parameter. It can be used for search the modification date. Returns products modified after the specified date. Format: 2019-01-01T00:00:00."
                    }
                },
                {
                    "name": "updatedBefore",
                    "description": {
                        "en": "Search parameter. It can be used for search the modification date. Returns products modified before the specified date. Format: 2019-01-01T00:00:00."
                    }
                },
                {
                    "name": "published",
                    "description": {
                        "en": "Search parameter. You can filter by product status. Values: 0 (Disabled), 1 (Enabled), 2 (Deprecated). By default, it lists all products."
                    }
                },
                {
                    "name": "parentProductId",
                    "description": {
                        "en": "Search parameter. According to the parent product innerId, it will filter to product having the same parent."
                    }
                }
            ],
            "example": {
                "url": "\/productExtend?full=1&page=0&limit=2",
                "statusCode": "200"
            }
        },
        "post": {
            "method": "POST",
            "url": {
                "en": "\/productExtend[\/product id]"
            },
            "description": {
                "en": "It is used to create or modify. Resources marked as Related Resources are always created."
            },
            "parameters": [
                {
                    "name": "sku",
                    "description": {
                        "en": "Product SKU. It is used to identify the product to be modified. If a product with the specified SKU does not exist, an error is returned."
                    }
                }
            ],
            "example": {
                "url": "\/products",
                "statusCode": "200 or 201"
            }
        },
